@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix : <http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#> .
@prefix food: <http://www.w3.org/TR/2003/PR-owl-guide-20031209/food#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine>
    a owl:Ontology ;
    rdfs:comment "An example OWL ontology", """Derived from the DAML Wine ontology at 
      http://ontolingua.stanford.edu/doc/chimaera/ontologies/wines.daml
      Substantially changed, in particular the Region based relations.
    """ ;
    rdfs:label "Wine Ontology" ;
    owl:imports <http://www.w3.org/TR/2003/PR-owl-guide-20031209/food> ;
    owl:priorVersion <http://www.w3.org/TR/2003/CR-owl-guide-20030818/wine> .

<http://www.w3.org/TR/2003/CR-owl-guide-20030818/wine>
    a owl:Ontology .

:Wine
    a owl:Class ;
    rdfs:label "vin"@fr, "wine"@en ;
    rdfs:subClassOf food:PotableLiquid, [
        a owl:Restriction ;
        owl:cardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty :hasMaker
    ], [
        a owl:Restriction ;
        owl:allValuesFrom :Winery ;
        owl:onProperty :hasMaker
    ], [
        a owl:Restriction ;
        owl:minCardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty :madeFromGrape
    ], [
        a owl:Restriction ;
        owl:cardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty :hasSugar
    ], [
        a owl:Restriction ;
        owl:cardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty :hasFlavor
    ], [
        a owl:Restriction ;
        owl:cardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty :hasBody
    ], [
        a owl:Restriction ;
        owl:cardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty :hasColor
    ], [
        a owl:Restriction ;
        owl:onProperty :locatedIn ;
        owl:someValuesFrom :Region
    ] .

:Vintage
    a owl:Class ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:cardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty :hasVintageYear
    ] .

:WineGrape
    a owl:Class ;
    rdfs:subClassOf food:Grape .

:WhiteWine
    a owl:Class ;
    owl:intersectionOf [
        rdf:first :Wine ;
        rdf:rest [
            rdf:first [
                a owl:Restriction ;
                owl:hasValue :White ;
                owl:onProperty :hasColor
            ] ;
            rdf:rest rdf:nil
        ]
    ] .

:WhiteTableWine
    a owl:Class ;
    owl:intersectionOf [
        rdf:first :TableWine ;
        rdf:rest [
            rdf:first [
                a owl:Restriction ;
                owl:hasValue :White ;
                owl:onProperty :hasColor
            ] ;
            rdf:rest rdf:nil
        ]
    ] .

:TableWine
    a owl:Class ;
    owl:intersectionOf [
        rdf:first :Wine ;
        rdf:rest [
            rdf:first [
                a owl:Restriction ;
                owl:hasValue :Dry ;
                owl:onProperty :hasSugar
            ] ;
            rdf:rest rdf:nil
        ]
    ] .

:WhiteNonSweetWine
    a owl:Class ;
    owl:intersectionOf [
        rdf:first :WhiteWine ;
        rdf:rest [
            rdf:first [
                a owl:Restriction ;
                owl:allValuesFrom [
                    a owl:Class ;
                    owl:oneOf [
                        rdf:first :Dry ;
                        rdf:rest [
                            rdf:first :OffDry ;
                            rdf:rest rdf:nil
                        ]
                    ]
                ] ;
                owl:onProperty :hasSugar
            ] ;
            rdf:rest rdf:nil
        ]
    ] .

:Dry
    a owl:Thing, :WineSugar .

:OffDry
    a owl:Thing, :WineSugar ;
    owl:differentFrom :Dry, :Sweet .

:WhiteLoire
    a owl:Class ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:allValuesFrom [
            a owl:Class ;
            owl:oneOf [
                rdf:first :CheninBlancGrape ;
                rdf:rest [
                    rdf:first :PinotBlancGrape ;
                    rdf:rest [
                        rdf:first :SauvignonBlancGrape ;
                        rdf:rest rdf:nil
                    ]
                ]
            ]
        ] ;
        owl:onProperty :madeFromGrape
    ] ;
    owl:intersectionOf [
        rdf:first :Loire ;
        rdf:rest [
            rdf:first :WhiteWine ;
            rdf:rest rdf:nil
        ]
    ] .

:Loire
    a owl:Class ;
    owl:intersectionOf [
        rdf:first :Wine ;
        rdf:rest [
            rdf:first [
                a owl:Restriction ;
                owl:hasValue :LoireRegion ;
                owl:onProperty :locatedIn
            ] ;
            rdf:rest rdf:nil
        ]
    ] .

:CheninBlancGrape
    a owl:Thing, :WineGrape .

:PinotBlancGrape
    a owl:Thing, :WineGrape .

:SauvignonBlancGrape
    a owl:Thing, :WineGrape .

:WhiteBurgundy
    a owl:Class ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:hasValue :ChardonnayGrape ;
        owl:onProperty :madeFromGrape
    ], [
        a owl:Restriction ;
        owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty :madeFromGrape
    ] ;
    owl:intersectionOf [
        rdf:first :Burgundy ;
        rdf:rest [
            rdf:first :WhiteWine ;
            rdf:rest rdf:nil
        ]
    ] .

:Burgundy
    a owl:Class ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:hasValue :Dry ;
        owl:onProperty :hasSugar
    ] ;
    owl:intersectionOf [
        rdf:first :Wine ;
        rdf:rest [
            rdf:first [
                a owl:Restriction ;
                owl:hasValue :BourgogneRegion ;
                owl:onProperty :locatedIn
            ] ;
            rdf:rest rdf:nil
        ]
    ] .

:WhiteBordeaux
    a owl:Class ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:allValuesFrom [
            a owl:Class ;
            owl:oneOf [
                rdf:first :SemillonGrape ;
                rdf:rest [
                    rdf:first :SauvignonBlancGrape ;
                    rdf:rest rdf:nil
                ]
            ]
        ] ;
        owl:onProperty :madeFromGrape
    ] ;
    owl:intersectionOf [
        rdf:first :Bordeaux ;
        rdf:rest [
            rdf:first :WhiteWine ;
            rdf:rest rdf:nil
        ]
    ] .

:Bordeaux
    a owl:Class ;
    owl:intersectionOf [
        rdf:first :Wine ;
        rdf:rest [
            rdf:first [
                a owl:Restriction ;
                owl:hasValue :BordeauxRegion ;
                owl:onProperty :locatedIn
            ] ;
            rdf:rest rdf:nil
        ]
    ] .

:SemillonGrape
    a owl:Thing, :WineGrape .

:Region
    a owl:Class .

:locatedIn
    a owl:ObjectProperty, owl:TransitiveProperty ;
    rdfs:domain owl:Thing ;
    rdfs:range :Region .

:adjacentRegion
    a owl:ObjectProperty, owl:SymmetricProperty ;
    rdfs:domain :Region ;
    rdfs:range :Region .

:VintageYear
    a owl:Class .

:yearValue
    a owl:DatatypeProperty ;
    rdfs:domain :VintageYear ;
    rdfs:range xsd:positiveInteger .

:Year1998
    a :VintageYear ;
    :yearValue "1998"^^xsd:positiveInteger .

:hasVintageYear
    a owl:FunctionalProperty, owl:ObjectProperty ;
    rdfs:domain :Vintage ;
    rdfs:range :VintageYear .

:madeFromGrape
    a owl:ObjectProperty ;
    rdfs:domain :Wine ;
    rdfs:range :WineGrape ;
    rdfs:subPropertyOf food:madeFromFruit .

:madeIntoWine
    a owl:ObjectProperty ;
    owl:inverseOf :madeFromGrape .

:hasWineDescriptor
    a owl:ObjectProperty ;
    rdfs:domain :Wine ;
    rdfs:range :WineDescriptor .

:hasSugar
    a owl:FunctionalProperty, owl:ObjectProperty ;
    rdfs:range :WineSugar ;
    rdfs:subPropertyOf :hasWineDescriptor .

:hasBody
    a owl:FunctionalProperty, owl:ObjectProperty ;
    rdfs:range :WineBody ;
    rdfs:subPropertyOf :hasWineDescriptor .

:hasFlavor
    a owl:FunctionalProperty, owl:ObjectProperty ;
    rdfs:range :WineFlavor ;
    rdfs:subPropertyOf :hasWineDescriptor .

:hasColor
    a owl:FunctionalProperty, owl:ObjectProperty ;
    rdfs:domain :Wine ;
    rdfs:range :WineColor ;
    rdfs:subPropertyOf :hasWineDescriptor .

:hasMaker
    a owl:FunctionalProperty, owl:ObjectProperty .

:producesWine
    a owl:ObjectProperty ;
    owl:inverseOf :hasMaker .

:Zinfandel
    a owl:Class ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:hasValue :Red ;
        owl:onProperty :hasColor
    ], [
        a owl:Restriction ;
        owl:hasValue :Dry ;
        owl:onProperty :hasSugar
    ], [
        a owl:Restriction ;
        owl:allValuesFrom [
            a owl:Class ;
            owl:oneOf [
                rdf:first :Full ;
                rdf:rest [
                    rdf:first :Medium ;
                    rdf:rest rdf:nil
                ]
            ]
        ] ;
        owl:onProperty :hasBody
    ], [
        a owl:Restriction ;
        owl:allValuesFrom [
            a owl:Class ;
            owl:oneOf [
                rdf:first :Moderate ;
                rdf:rest [
                    rdf:first :Strong ;
                    rdf:rest rdf:nil
                ]
            ]
        ] ;
        owl:onProperty :hasFlavor
    ] ;
    owl:intersectionOf [
        rdf:first :Wine ;
        rdf:rest [
            rdf:first [
                a owl:Restriction ;
                owl:hasValue :ZinfandelGrape ;
                owl:onProperty :madeFromGrape
            ] ;
            rdf:rest [
                rdf:first [
                    a owl:Restriction ;
                    owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                    owl:onProperty :madeFromGrape
                ] ;
                rdf:rest rdf:nil
            ]
        ]
    ] .

:Full
    a owl:Thing, :WineBody .

:Medium
    a owl:Thing, :WineBody .

:Moderate
    a owl:Thing, :WineFlavor .

:Strong
    a owl:Thing, :WineFlavor .

:Winery
    a owl:Class .

:WineDescriptor
    a owl:Class ;
    rdfs:comment "Made WineDescriptor unionType of tastes and color" ;
    owl:unionOf [
        rdf:first :WineTaste ;
        rdf:rest [
            rdf:first :WineColor ;
            rdf:rest rdf:nil
        ]
    ] .

:WineTaste
    a owl:Class ;
    rdfs:subClassOf :WineDescriptor .

:WineColor
    a owl:Class ;
    rdfs:subClassOf :WineDescriptor ;
    owl:oneOf [
        rdf:first :White ;
        rdf:rest [
            rdf:first :Rose ;
            rdf:rest [
                rdf:first :Red ;
                rdf:rest rdf:nil
            ]
        ]
    ] .

:White
    a owl:Thing, :WineColor .

:Rose
    a owl:Thing, :WineColor .

:Red
    a owl:Thing, :WineColor .

:WineSugar
    a owl:Class ;
    rdfs:subClassOf :WineTaste ;
    owl:oneOf [
        rdf:first :Sweet ;
        rdf:rest [
            rdf:first :OffDry ;
            rdf:rest [
                rdf:first :Dry ;
                rdf:rest rdf:nil
            ]
        ]
    ] .

:Sweet
    a owl:Thing, :WineSugar ;
    owl:differentFrom :Dry .

:WineFlavor
    a owl:Class ;
    rdfs:subClassOf :WineTaste ;
    owl:oneOf [
        rdf:first :Delicate ;
        rdf:rest [
            rdf:first :Moderate ;
            rdf:rest [
                rdf:first :Strong ;
                rdf:rest rdf:nil
            ]
        ]
    ] .

:Delicate
    a owl:Thing, :WineFlavor .

:WineBody
    a owl:Class ;
    rdfs:subClassOf :WineTaste ;
    owl:oneOf [
        rdf:first :Light ;
        rdf:rest [
            rdf:first :Medium ;
            rdf:rest [
                rdf:first :Full ;
                rdf:rest rdf:nil
            ]
        ]
    ] .

:Light
    a owl:Thing, :WineBody .

:USRegion
    a :Region .

:Tours
    a owl:Class ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:hasValue :CheninBlancGrape ;
        owl:onProperty :madeFromGrape
    ], [
        a owl:Restriction ;
        owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty :madeFromGrape
    ] ;
    owl:intersectionOf [
        rdf:first :Loire ;
        rdf:rest [
            rdf:first [
                a owl:Restriction ;
                owl:hasValue :ToursRegion ;
                owl:onProperty :locatedIn
            ] ;
            rdf:rest rdf:nil
        ]
    ] .

:SweetWine
    a owl:Class ;
    owl:intersectionOf [
        rdf:first :Wine ;
        rdf:rest [
            rdf:first [
                a owl:Restriction ;
                owl:hasValue :Sweet ;
                owl:onProperty :hasSugar
            ] ;
            rdf:rest rdf:nil
        ]
    ] .

:SweetRiesling
    a owl:Class ;
    rdfs:subClassOf :DessertWine, [
        a owl:Restriction ;
        owl:hasValue :Full ;
        owl:onProperty :hasBody
    ], [
        a owl:Restriction ;
        owl:allValuesFrom [
            a owl:Class ;
            owl:oneOf [
                rdf:first :Moderate ;
                rdf:rest [
                    rdf:first :Strong ;
                    rdf:rest rdf:nil
                ]
            ]
        ] ;
        owl:onProperty :hasFlavor
    ] ;
    owl:intersectionOf [
        rdf:first :Riesling ;
        rdf:rest [
            rdf:first [
                a owl:Restriction ;
                owl:hasValue :Sweet ;
                owl:onProperty :hasSugar
            ] ;
            rdf:rest rdf:nil
        ]
    ] .

:Riesling
    a owl:Class ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:hasValue :White ;
        owl:onProperty :hasColor
    ] ;
    owl:intersectionOf [
        rdf:first :Wine ;
        rdf:rest [
            rdf:first [
                a owl:Restriction ;
                owl:hasValue :RieslingGrape ;
                owl:onProperty :madeFromGrape
            ] ;
            rdf:rest [
                rdf:first [
                    a owl:Restriction ;
                    owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                    owl:onProperty :madeFromGrape
                ] ;
                rdf:rest rdf:nil
            ]
        ]
    ] .

:StEmilion
    a owl:Class ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:hasValue :Red ;
        owl:onProperty :hasColor
    ], [
        a owl:Restriction ;
        owl:hasValue :Strong ;
        owl:onProperty :hasFlavor
    ], [
        a owl:Restriction ;
        owl:hasValue :CabernetSauvignonGrape ;
        owl:onProperty :madeFromGrape
    ], [
        a owl:Restriction ;
        owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty :madeFromGrape
    ] ;
    owl:intersectionOf [
        rdf:first :Bordeaux ;
        rdf:rest [
            rdf:first [
                a owl:Restriction ;
                owl:hasValue :StEmilionRegion ;
                owl:onProperty :locatedIn
            ] ;
            rdf:rest rdf:nil
        ]
    ] .

:SemillonOrSauvignonBlanc
    a owl:Class ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:hasValue :White ;
        owl:onProperty :hasColor
    ], [
        a owl:Restriction ;
        owl:allValuesFrom [
            a owl:Class ;
            owl:oneOf [
                rdf:first :Medium ;
                rdf:rest [
                    rdf:first :Full ;
                    rdf:rest rdf:nil
                ]
            ]
        ] ;
        owl:onProperty :hasBody
    ] ;
    owl:intersectionOf [
        rdf:first :Wine ;
        rdf:rest [
            rdf:first [
                a owl:Restriction ;
                owl:allValuesFrom [
                    a owl:Class ;
                    owl:oneOf [
                        rdf:first :SemillonGrape ;
                        rdf:rest [
                            rdf:first :SauvignonBlancGrape ;
                            rdf:rest rdf:nil
                        ]
                    ]
                ] ;
                owl:onProperty :madeFromGrape
            ] ;
            rdf:rest rdf:nil
        ]
    ] .

:Semillon
    a owl:Class ;
    owl:intersectionOf [
        rdf:first :SemillonOrSauvignonBlanc ;
        rdf:rest [
            rdf:first [
                a owl:Restriction ;
                owl:hasValue :SemillonGrape ;
                owl:onProperty :madeFromGrape
            ] ;
            rdf:rest [
                rdf:first [
                    a owl:Restriction ;
                    owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                    owl:onProperty :madeFromGrape
                ] ;
                rdf:rest rdf:nil
            ]
        ]
    ] .

:SauvignonBlanc
    a owl:Class ;
    owl:intersectionOf [
        rdf:first :SemillonOrSauvignonBlanc ;
        rdf:rest [
            rdf:first [
                a owl:Restriction ;
                owl:hasValue :SauvignonBlancGrape ;
                owl:onProperty :madeFromGrape
            ] ;
            rdf:rest [
                rdf:first [
                    a owl:Restriction ;
                    owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                    owl:onProperty :madeFromGrape
                ] ;
                rdf:rest rdf:nil
            ]
        ]
    ] .

:Sauternes
    a owl:Class ;
    rdfs:subClassOf :Bordeaux, :LateHarvest, [
        a owl:Restriction ;
        owl:hasValue :SauterneRegion ;
        owl:onProperty :locatedIn
    ], [
        a owl:Restriction ;
        owl:hasValue :Medium ;
        owl:onProperty :hasBody
    ], [
        a owl:Restriction ;
        owl:hasValue :White ;
        owl:onProperty :hasColor
    ] .

:Sancerre
    a owl:Class ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:hasValue :Medium ;
        owl:onProperty :hasBody
    ], [
        a owl:Restriction ;
        owl:hasValue :OffDry ;
        owl:onProperty :hasSugar
    ], [
        a owl:Restriction ;
        owl:hasValue :Delicate ;
        owl:onProperty :hasFlavor
    ], [
        a owl:Restriction ;
        owl:hasValue :SauvignonBlancGrape ;
        owl:onProperty :madeFromGrape
    ], [
        a owl:Restriction ;
        owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty :madeFromGrape
    ] ;
    owl:intersectionOf [
        rdf:first :Loire ;
        rdf:rest [
            rdf:first [
                a owl:Restriction ;
                owl:hasValue :SancerreRegion ;
                owl:onProperty :locatedIn
            ] ;
            rdf:rest rdf:nil
        ]
    ] .

:RoseWine
    a owl:Class ;
    owl:intersectionOf [
        rdf:first :Wine ;
        rdf:rest [
            rdf:first [
                a owl:Restriction ;
                owl:hasValue :Rose ;
                owl:onProperty :hasColor
            ] ;
            rdf:rest rdf:nil
        ]
    ] .

:RedWine
    a owl:Class ;
    owl:intersectionOf [
        rdf:first :Wine ;
        rdf:rest [
            rdf:first [
                a owl:Restriction ;
                owl:hasValue :Red ;
                owl:onProperty :hasColor
            ] ;
            rdf:rest rdf:nil
        ]
    ] .

:RedTableWine
    a owl:Class ;
    owl:intersectionOf [
        rdf:first :TableWine ;
        rdf:rest [
            rdf:first [
                a owl:Restriction ;
                owl:hasValue :Red ;
                owl:onProperty :hasColor
            ] ;
            rdf:rest rdf:nil
        ]
    ] .

:RedBurgundy
    a owl:Class ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:hasValue :PinotNoirGrape ;
        owl:onProperty :madeFromGrape
    ], [
        a owl:Restriction ;
        owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty :madeFromGrape
    ] ;
    owl:intersectionOf [
        rdf:first :Burgundy ;
        rdf:rest [
            rdf:first :RedWine ;
            rdf:rest rdf:nil
        ]
    ] .

:RedBordeaux
    a owl:Class ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:allValuesFrom [
            a owl:Class ;
            owl:oneOf [
                rdf:first :CabernetSauvignonGrape ;
                rdf:rest [
                    rdf:first :MerlotGrape ;
                    rdf:rest rdf:nil
                ]
            ]
        ] ;
        owl:onProperty :madeFromGrape
    ] ;
    owl:intersectionOf [
        rdf:first :Bordeaux ;
        rdf:rest [
            rdf:first :RedWine ;
            rdf:rest rdf:nil
        ]
    ] .

:CabernetSauvignonGrape
    a owl:Thing, :WineGrape .

:MerlotGrape
    a owl:Thing, :WineGrape .

:Port
    a owl:Class ;
    rdfs:subClassOf :RedWine, [
        a owl:Restriction ;
        owl:hasValue :PortugalRegion ;
        owl:onProperty :locatedIn
    ], [
        a owl:Restriction ;
        owl:hasValue :Full ;
        owl:onProperty :hasBody
    ], [
        a owl:Restriction ;
        owl:hasValue :Strong ;
        owl:onProperty :hasFlavor
    ], [
        a owl:Restriction ;
        owl:hasValue :Sweet ;
        owl:onProperty :hasSugar
    ] .

:PinotNoir
    a owl:Class ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:hasValue :Red ;
        owl:onProperty :hasColor
    ] ;
    owl:intersectionOf [
        rdf:first :Wine ;
        rdf:rest [
            rdf:first [
                a owl:Restriction ;
                owl:hasValue :PinotNoirGrape ;
                owl:onProperty :madeFromGrape
            ] ;
            rdf:rest [
                rdf:first [
                    a owl:Restriction ;
                    owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                    owl:onProperty :madeFromGrape
                ] ;
                rdf:rest rdf:nil
            ]
        ]
    ] .

:PinotBlanc
    a owl:Class ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:hasValue :White ;
        owl:onProperty :hasColor
    ] ;
    owl:intersectionOf [
        rdf:first :Wine ;
        rdf:rest [
            rdf:first [
                a owl:Restriction ;
                owl:hasValue :PinotBlancGrape ;
                owl:onProperty :madeFromGrape
            ] ;
            rdf:rest [
                rdf:first [
                    a owl:Restriction ;
                    owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                    owl:onProperty :madeFromGrape
                ] ;
                rdf:rest rdf:nil
            ]
        ]
    ] .

:PetiteSyrah
    a owl:Class ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:hasValue :Red ;
        owl:onProperty :hasColor
    ], [
        a owl:Restriction ;
        owl:hasValue :Dry ;
        owl:onProperty :hasSugar
    ], [
        a owl:Restriction ;
